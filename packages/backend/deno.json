{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "checkJs": true,
    "exactOptionalPropertyTypes": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  },
  "imports": {
    "@deno/cache-dir": "jsr:@deno/cache-dir@0.16.0",
    "@prisma/client": "./generated/client/index.cjs",
    "@resvg/resvg-js": "npm:@resvg/resvg-js@2.6.2",
    "@scout/data": "../data/src/index.ts",
    "@scout/report": "../report/src/index.ts",
    "@std/assert": "jsr:@std/assert@1.0.10",
    "@std/encoding": "jsr:@std/encoding@1.0.6",
    "@std/testing": "jsr:@std/testing@1.0.8",
    "@std/dotenv": "jsr:@std/dotenv@0.225.3",
    "@total-typescript/ts-reset": "https://esm.sh/@total-typescript/ts-reset@0.6.1?pin=v135",
    "async_hooks": "node:async_hooks",
    "buffer": "node:buffer",
    "child_process": "node:child_process",
    "cron": "https://esm.sh/cron@3.5.0",
    "crypto": "node:crypto",
    "date-fns": "https://esm.sh/date-fns@4.1.0",
    "discord.js": "npm:discord.js@14.17.3",
    "dns": "node:dns",
    "dns/promises": "node:dns/promises",
    "env-var": "https://esm.sh/env-var@7.5.0",
    "dotenv@16.4.7/package.json?module": "https://esm.sh/empty-npm-package@1.0.0",
    "events": "node:events",
    "fs": "node:fs",
    "fs/promises": "node:fs/promises",
    "http": "node:http",
    "http2": "node:http2",
    "https": "node:https",
    "net": "node:net",
    "prisma": "npm:prisma@6.3.0",
    "querystring": "node:querystring",
    "react": "https://esm.sh/react@19.0.0",
    "readline": "node:readline",
    "readline/promises": "node:readline/promises",
    "remeda": "https://esm.sh/remeda@2.20.0",
    "satori": "npm:satori@0.12.1",
    "sentry/": "https://deno.land/x/sentry@8.53.0/",
    "sqlite": "jsr:@db/sqlite@0.12.0",
    "stream": "node:stream",
    "stream/consumers": "node:stream/consumers",
    "stream/promises": "node:stream/promises",
    "stream/web": "node:stream/web",
    "tls": "node:tls",
    "ts-pattern": "https://esm.sh/ts-pattern@5.6.2",
    "twisted": "https://esm.sh/twisted@1.61.5",
    "twisted/": "https://esm.sh/twisted@1.61.5/",
    "url": "node:url",
    "uuid": "https://esm.sh/uuid@11.0.5",
    "yoga-wasm-web": "npm:yoga-wasm-web@0.3.3",
    "zod": "https://esm.sh/zod@3.24.1",
    "zod-validation-error": "https://esm.sh/zod-validation-error@3.4.0"
  },
  "tasks": {
    "check": "deno check src/index.ts",
    "check:watch": "deno run -A https://deno.land/x/denon/denon.ts check",
    "run": "deno run -A --unstable-ffi src/index.ts",
    "test:update": "deno test --allow-all -- --update"
  },
  "lock": true
}
